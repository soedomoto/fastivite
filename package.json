{
  "name": "fastivite",
  "version": "1.0.6",
  "author": "Soedomoto <soedomoto@gmail.com>",
  "description": "Create monolithic app made easy by using combination of fastify and vite",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "fastivite": "./dist/cli.js"
  },
  "scripts": {
    "fastivite": "./dist/cli.js",
    "dev": "swc --strip-leading-paths --delete-dir-on-start -s -w -d dist src",
    "build": "swc --strip-leading-paths --delete-dir-on-start -d dist src",
    "postbuild": "concurrently npm:typecheck",
    "format": "prettier -w src test",
    "typecheck": "tsc -p . --emitDeclarationOnly"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/middie": "^8.3.1",
    "@fastify/restartable": "^2.3.0",
    "@swc/cli": "^0.3.12",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.2",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "fastify": "^4.27.0",
    "fastify-print-routes": "^3.2.0",
    "glob": "^10.4.1",
    "lodash": "^4.17.21",
    "sirv": "^2.0.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "esbuild": "^0.21.5",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "commander": "^12.1.0"
  }
}